              <a class="more" href="/flash.html">animez vos sites</a>
              <div class="clear"></div>
            </div><!-- END slide -->
            <div class="slide" data-nav="fifth">
              <%= image_tag "szkolenie.jpg", width: 406, height: 296, alt: "img1" %>
